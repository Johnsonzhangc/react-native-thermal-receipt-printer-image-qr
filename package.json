{
  "name": "react-native-thermal-receipt-printer-image-qr",
  "version": "0.1.11",
  "description": "Fork of react-native-thermal-receipt-printer. A React Native Library to support USB/BLE/Net printer",
  "main": "dist/index.js",
  "source": "dist/index.js",
  "scripts": {
    "pods": "cd example && pod-install --quiet",
    "example": "yarn --cwd example",
    "bootstrap": "yarn example && yarn && yarn pods",
    "build": "rimraf ./dist && tsc",
    "test": "jshint hello.js",
    "prepare": "bob build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Johnsonzhangc/react-native-thermal-receipt-printer-image-qr.git#codepage"
  },
  "files": [
    "dist",
    "dist",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "src",
    "android",
    "ios",
    "react-native-thermal-receipt-printer-image-qr.podspec"
  ],
  "keywords": [
    "react-native",
    "thermal",
    "printer",
    "bluetooth",
    "net",
    "printer",
    "rn",
    "android",
    "image",
    "qr",
    "base64",
    "receipt",
    "bill",
    "invoice"
  ],
  "dependencies": {
    "buffer": "^5.6.0",
    "iconv-lite": "^0.6.2",
    "string_decoder": "^1.3.0",
    "esc-pos-encoder": "1.3.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "^14.6.0",
    "@types/react": "^16.9.46",
    "@types/react-native": "^0.63.9",
    "@types/react-test-renderer": "^16.9.3",
    "jshint": "^2.13.1",
    "pod-install": "^0.1.10",
    "react": "^16.13.1",
    "react-native": "^0.64.1",
    "react-native-builder-bob": "^0.23.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.0.2"
  },
  "peerDependencies": {
    "react-native-ping": "*"
  },
  "author": "thiendangit",
  "homepage": "https://github.com/thiendangit/react-native-thermal-receipt-printer-image-qr#readme",
  "license": "ISC",
  "react-native": "dist/index.js",
  "module": "dist/module/index.js",
  "types": "dist/typescript/index.d.ts",
  "react-native-builder-bob": {
    "source": "dist",
    "output": "dist",
    "targets": [
      "module"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ]
}
